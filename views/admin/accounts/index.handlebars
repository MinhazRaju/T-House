<div class="container-fluid">
  <!-- Breadcrumbs-->

  <div class="container-fluid">
    <!-- Breadcrumbs-->



    <div class="row">


<h1>{{id}}</h1>





      {{#if fetchCashInEditData}}
      <div class="col-md-6">

        

        <form action="/admin/cashInReport/update/{{fetchCashInEditData.id}}" method="post">


          <div class="form-group">
            <label for="exampleInputPassword1"><b>SECTOR</b></label>
            <select name="sector" id="stauts" class="form-control">
              {{#select fetchCashInEditData.seeder}}
              {{#each fetchCashSeederData}}
              <option value="{{id}}">{{seeder}}</option>
              {{/each}}
              {{/select}}
            </select>
          </div>



          <div class="form-group">
            <label for="body"><b>AMOUNT</b></label>
            <input class="form-control" type="number" name="amount" value="{{fetchCashInEditData.amount}}">
          </div>


          <div class="form-group">
            <label for="body"><b>DESCRIPTION</b></label>
            <input type="text" name="body" class="form-control" value="{{fetchCashInEditData.description}}">
          </div>

          


          <button type="submit" class="btn btn-primary">update</button>
          <a href="/admin/cashInReport" class="btn btn-danger">cancle</a>
        </form>

      </div>


      {{else}}

      <div class="col-md-6">


      
      <h6>Entry Point</h6>
      <form action="/admin/createEvent" method="post" enctype="multipart/form-data">

        <div class="form-group">
          <label for="exampleInputPassword1"><b>SECTOR</b></label>

          {{#if s}}
             <select name="sector" id="stauts" class="form-control">
               
            {{#each s}}
            <option value={{id}}><span>{{seeder}}</span></option>
            {{/each}}
        </select>
            {{else}}

          <select name="sector" id="stauts" class="form-control">
            {{#each seederFetch}}
            <option value={{id}}><span>{{seeder}}</span></option>
            {{/each}}
          </select>
            {{/if}}
          

        </div>



        <div class="form-group">
          <label for="body"><b>AMOUNT</b></label>
          <input class="form-control" type="number" name="amount">
        </div>


        <div class="form-group">
          <label for="body"><b>DESCRIPTION</b></label>
          <input type="text" name="body" class="form-control">
        </div>


        <button type="submit" class="btn btn-primary">Submit</button>
      </form>


      </div>
      






 


    {{/if}}

  {{#if fetchCashInEditData}}


    {{else}}
    <div class="col-sm-6">


  




      <div class="card">
        <div class="card-body" style="margin:45px">
          <h5 class="card-title">Transaction Details</h5>

                <table class="table table-bordered">  
  <tbody>
    <tr>
      
      <td>Blance</td>
      <td><b>&#x09F3; {{total}}</b></td>
      
    </tr>
    <tr>
      <td>Cash amount</td>
      <td><b>&#x09F3; {{cashamount}}</b></td>     
    </tr>
     <tr>
      <td>Bank amount</td>
      <td><b>&#x09F3; {{bankamount}}</b></td>
     
    </tr>
    <tr>
      <td>Total Entry</td>
      <td><b>{{count}}</b></td>
     
    </tr>  

    <tr>
      <td>Cash Entry</td>
      <td><b>{{cash}}</b></td>
     
    </tr>
      
    
    <tr>
      <td>Bank Entry</td>
      <td><b>{{bank}}</b></td>
     
    </tr> 
     
  
   
  </tbody>
</table>


          <div class="list-group">
           
          </div>
        </div>
      </div>
    </div>
  {{/if}}








  </div>



</div>





<br>  


{{#if fetchCashInEditData}}


{{else}}
<a style="margin-left:13px" href="/admin/cashInReport/csv" class="btn btn-success">Export csv</a>
<br>
<br>  
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Date</th>
      <th scope="col">Ref Cash</th>
      <th scope="col">Amount</th>
      <th scope="col">Description</th>
      <th scope='col'>Edit</th>

    </tr>
  </thead>
  <tbody>



    <tr>

      {{#each fetchCashInData}}


      <th scope="row">{{random}}</th>
      <td><b>{{getDate date 'Do-Mo-YYYY' }}</b><br><span style="color:indigo;">{{getDate date 'LTS'}}</span></td>
      <td><b>{{seeder.seeder}}</b></td>
      <td><b>&#x09F3; {{amount}}</b></td>
      <td>{{description}}</td>
      <td><a href="/admin/cashInReport/edit/{{id}}" class="btn btn-light">Eidt</i></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>
<hr>

{{/if}}




</div>