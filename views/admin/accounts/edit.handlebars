     <h1>Edit Event</h1>



  <form action="/admin/createEvent" method="post" enctype="multipart/form-data" >

  {{#if fetchCashInData}}

  <div class="form-group">
    <label for="exampleInputPassword1"><b>SECTOR</b></label>
    
    {{#select fetchCashInData.sector }}
    
      <select name="sector" id="stauts" class="form-control">              
            <option value="cash">Cash</option>  
            <option value="bank">Bank</option>     
      </select>

    {{/select}}


  </div>



  <div class="form-group">
        <label for="body"><b>AMOUNT</b></label>
        <input class="form-control" type="number" name="amount" value="{{fetchCashInData.amount}}">
  </div>









    <div class="form-group">
        <label for="body"><b>DESCRIPTION</b></label>
        <input type="text" name="body" class="form-control" value="{{fetchCashInData.description}}">
    </div>




    <div class="form-group">
        <label for="files">File Upload</label>
       <input type="file" name="filedata">
    </div>





  <button type="submit" class="btn btn-primary">Submit</button>
</form>

{{/if}}




    </div>