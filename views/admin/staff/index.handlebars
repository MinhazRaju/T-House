<div class="container-fluid">
  <!-- Breadcrumbs-->

  <div class="container-fluid">
    <!-- Breadcrumbs-->



    <div class="row">


<h1>{{id}}</h1>





      {{#if fetchCashInEditData}}
      <div class="col-md-6">

        

        <form action="/admin/cashOutReport/update/{{fetchCashInEditData.id}}" method="post">


          <div class="form-group">
            <label for="body"><b>STAFF NAME</b></label>
            <input class="form-control" type="text" name="amount" value="{{fetchCashInEditData.amount}}">
          </div>



          <div class="form-group">
            <label for="body"><b>SALARY</b></label>
            <input class="form-control" type="number" name="amount" value="{{fetchCashInEditData.amount}}">
          </div>


          <div class="form-group">
            <label for="body"><b>POSITION</b></label>
            <input type="text" name="body" class="form-control" value="{{fetchCashInEditData.description}}">
          </div>

          


          <button type="submit" class="btn btn-primary">update</button>
          <a href="/admin/cashOutReport" class="btn btn-danger">cancle</a>
        </form>

      </div>


      {{else}}

      <div class="col-md-6">


      
      <h6>Entry Point</h6>
      <form action="/admin/cashOutReport/update/{{fetchCashInEditData.id}}" method="post">


          <div class="form-group">
            <label for="body"><b>STAFF NAME</b></label>
            <input class="form-control" type="text" name="amount" value="{{fetchCashInEditData.amount}}">
          </div>



          <div class="form-group">
            <label for="body"><b>SALARY</b></label>
            <input class="form-control" type="number" name="amount" value="{{fetchCashInEditData.amount}}">
          </div>


          <div class="form-group">
            <label for="body"><b>POSITION</b></label>
            <input type="text" name="body" class="form-control" value="{{fetchCashInEditData.description}}">
          </div>

          


          <button type="submit" class="btn btn-success">submit</button>
         
        </form>


      </div>
      






 


    {{/if}}

  {{#if fetchCashInEditData}}


    {{else}}
    <div class="col-sm-6">


  




      <div class="card">
        <div class="card-body" style="margin:45px">
          <h5 class="card-title">Transaction Details</h5>

                <table class="table table-bordered">  
  <tbody>
    <tr>
      
      <td>Total Salary</td>
      <td><b>&#x09F3; {{total}}</b></td>
      
    </tr>
    <tr>
      <td>Total Staff</td>
      <td><b>&#x09F3; {{cashamount}}</b></td>     
    </tr>
   
    <tr>
      <td>Cash Entry</td>
      <td><b>{{cash}}</b></td>
     
    </tr>
      
    
    <tr>
      <td>Bank Entry</td>
      <td><b>{{bank}}</b></td>
     
    </tr> 
     
  
   
  </tbody>
</table>


          <div class="list-group">
           
          </div>
        </div>
      </div>
    </div>
  {{/if}}








  </div>



</div>





<br>  


{{#if fetchCashInEditData}}


{{else}}
<a style="margin-left:13px" href="/admin/cashInReport/csv" class="btn btn-success">Export csv</a>
<br>
<br>  
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Date</th>
      <th scope="col">Salary</th>
      <th scope="col">Position</th>      
      <th scope='col'>Edit</th>
      <th scope='col'>Delete</th>


    </tr>
  </thead>
  <tbody>



    <tr>

      {{#each fetchCashInData}}


      <th scope="row">{{random}}</th>
      <td><b>{{getDate date 'Do-Mo-YYYY' }}</b><br><span style="color:indigo;">{{getDate date 'LTS'}}</span></td>
      <td><b>{{seeder.seeder}}</b></td>
      <td><b>&#x09F3; {{amount}}</b></td>
      <td>{{description}}</td>
      <td><a href="/admin/cashOutReport/edit/{{id}}" class="btn btn-light">Eidt</i></a></td>
      <td><a href="/admin/cashOutReport/edit/{{id}}" class="btn btn-light">Delete</i></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>
<hr>

{{/if}}




</div>